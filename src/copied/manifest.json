{
    "name": "Enecuum",
    "description": "Enecuum Wallet",
    "version": "0.0.1",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "background.js",
            "enqweb3.min.js",
            "jquery-3.5.1.min.js",
            "lockAccount.js"
        ]
    },
    "browser_action": {
        "default_title": "Enecuum Wallet",
        "default_popup": "popup.html",
        "default_icon": "enq.png"
    },
    "icons": {
        "16": "images/16.png",
        "32": "images/32.png",
        "48": "images/48.png",
        "128": "images/128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "contentscript.js"
            ],
            "run_at": "document_start",
            "all_frames": true
        }
    ],
    "permissions": [
        "storage",
        "idle",
        "activeTab",
        "tabs"
    ],
    "web_accessible_resources": [
        "inpage.js",
        "inPage.js"
    ]
}