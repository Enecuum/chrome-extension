{
  "name": "Enecuum extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; script-src-elem 'self' 'unsafe-eval';object-src 'self'",
  "browser_action": {
    "default_title": "enecuum",
    "default_popup":"popup.html",
    "default_icon": "ui/favicon.png"
  },
  "content_scripts":[
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "contentscript.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "permissions": [
    "storage",
    "idle"
  ],
  "web_accessible_resources": ["inpage.js"]
}